public class DiceSelect{

    private int value;


    // Laver en dice med x value
    // Scanner skal hente value, og kalde denne funktion med v√¶rdien fra scanneren som (som int)
    public CreateDice(int value) {

        this.value = value;

    }


    //Returnerer valuen af dice, skal bruges af RNG til Dice Roll
    public DiceReturn(){

        return this.value;


    }
    






























}